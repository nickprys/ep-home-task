using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ArrayReverse
{
    class Program
    {
        static void Main(string[] args)
        {
           // int[] nums2 = new int[4] { 1, 2, 3, 5 };
            {
                int[] array = { 1, 2, 3, 4, 5 };

                var s = string.Join(" ", array);
                Console.WriteLine(s);
                Console.ReadKey();
            }
        }
    }
}
